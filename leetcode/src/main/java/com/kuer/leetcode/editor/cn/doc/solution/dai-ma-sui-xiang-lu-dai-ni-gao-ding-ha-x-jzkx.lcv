          break
        } else {
          var left = i + 1
          var right = nums_tmp.length - 1
          while (left < right) {
            var sum = nums_tmp(i) + nums_tmp(left) + nums_tmp(right) // æ±‚ä¸‰æ•°ä¹‹å’Œ
            if (sum < 0) left += 1
            else if (sum > 0) right -= 1
            else {
              res += List(nums_tmp(i), nums_tmp(left), nums_tmp(right)) // å¦‚æœç­‰äº0 æ·»åŠ è¿›ç»“æœé›†
              // ä¸ºäº†é¿å…é‡å¤ï¼Œå¯¹leftå’Œrightè¿›è¡Œç§»åŠ¨
              while (left < right && nums_tmp(left) == nums_tmp(left + 1)) left += 1
              while (left < right && nums_tmp(right) == nums_tmp(right - 1)) right -= 1
              left += 1
              right -= 1
            }
